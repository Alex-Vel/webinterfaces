{
    "$schema": "http://json-schema.org/draft-07/schema",
    "$id": "http://example.com/example.json",
    "type": "object",
    "title": "The root schema",
    "description": "The root schema comprises the entire JSON document.",
    "default": {},
    "examples": [
        {
            "stationId": 1234,
            "measurementData": {
                "temp": 15,
                "humidity": 50,
                "windSpeed": 0.5
            },
            "time": "2020-01-25T00:00:00"
        }
    ],
    "required": [
        "stationId",
        "measurementData",
        "time"
    ],
    "properties": {
        "stationId": {
            "$id": "#/properties/stationId",
            "type": "integer",
            "title": "The stationId schema",
            "description": "An explanation about the purpose of this instance.",
            "default": 0,
            "examples": [
                1234
            ]
        },
        "measurementData": {
            "$id": "#/properties/measurementData",
            "type": "object",
            "title": "The measurementData schema",
            "description": "An explanation about the purpose of this instance.",
            "default": {},
            "examples": [
                {
                    "temp": 15,
                    "humidity": 50,
                    "windSpeed": 0.5
                }
            ],
            "required": [
                "temp",
                "humidity",
                "windSpeed"
            ],
            "properties": {
                "temp": {
                    "$id": "#/properties/measurementData/properties/temp",
                    "type": "integer",
                    "title": "The temp schema",
                    "description": "An explanation about the purpose of this instance.",
                    "default": 0,
                    "examples": [
                        15
                    ]
                },
                "humidity": {
                    "$id": "#/properties/measurementData/properties/humidity",
                    "type": "integer",
                    "title": "The humidity schema",
                    "description": "An explanation about the purpose of this instance.",
                    "default": 0,
                    "examples": [
                        50
                    ]
                },
                "windSpeed": {
                    "$id": "#/properties/measurementData/properties/windSpeed",
                    "type": "number",
                    "title": "The windSpeed schema",
                    "description": "An explanation about the purpose of this instance.",
                    "default": 0.0,
                    "examples": [
                        0.5
                    ]
                }
            },
            "additionalProperties": false
        },
        "time": {
            "$id": "#/properties/time",
            "type": "string",
            "title": "The time schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": [
                "2020-01-25T00:00:00"
            ]
        }
    },
    "additionalProperties": false
}